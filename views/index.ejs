<script src="js/index.js" type="text/javascript"></script>

<div class="row down-20">
	<div class="col-md-8 col-md-offset-2">
		<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
			<!-- Indicators -->
			<ol class="carousel-indicators">
				<li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
				<li data-target="#carousel-example-generic" data-slide-to="1"></li>
				<li data-target="#carousel-example-generic" data-slide-to="2"></li>
			</ol>

			<!-- Wrapper for slides -->
			<div class="carousel-inner">
				<div class="item active">
					<img src="/img/carousel1.jpg" id='car1' class="carousel-img col-md-12">
					<div class="carousel-caption">
						<%- item.description %>
					</div>
				</div>
				<div class="item">
					<img src="/img/carousel2.jpg" id='car2' class="carousel-img col-md-12">
					<div class="carousel-caption">
						<%- item.description %>
					</div>
				</div>
				<div class="item">
					<img src="/img/carousel3.jpg" id='car3' class="carousel-img col-md-12">
					<div class="carousel-caption">
						<%- item.description %>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

