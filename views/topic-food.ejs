<div class="row">
	<div class="pull-right topic-date">
		Published: <%= food[0].date %>
	</div>
</div>
<div class="row">
	<div class="col-md-offset-3 col-md-6 topic-title"><%= food[0].Restaurant %></div>
</div>

<!-- This is where we are going to put the carousel of images for the restaurant -->
<div class="row">
	
</div>

<div class="row">
	<div class="topic-description"><%= food[0].Description %></div>
</div>

<% for(var i=0; i<items.length; i++) { %>
<div class="topic-item col-md-12">
	<div class="row">
		<div class="topic-title"><%= item[i].Title %></div>
	</div>
	<div class="row">
		<img class="topic-item-picture" src="<%= item[i].Image %>">
	</div>
	<div class="row">
		<div class="topic-item-description"><%= item[i].Description %></div>
	</div>
</div>
<% } %>